{
	"expo": {
		"name": "hms-test",
		"slug": "hms-test",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.hms.test"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["./config-plugins/Permissions.js", "./config-plugins/ProjectGradle.js", "./config-plugins/AppGradle.js"]
	}
}
